                                                    <DoubleAnimation To="0.16" Storyboard.TargetName="comboBoxUIEffect" Storyboard.TargetProperty="Opacity" Duration="0:0:0.02"/>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </ControlTemplate.Triggers>
                                    <Border Name="comboBoxUI" CornerRadius="6" BorderThickness="1" BorderBrush="#28B4B4B4" Background="#FFFFFFFF">
