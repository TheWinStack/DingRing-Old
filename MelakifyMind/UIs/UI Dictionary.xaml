                                        <DoubleAnimation To="0.6" Storyboard.TargetName="buttonContent" Storyboard.TargetProperty="Opacity" Duration="0:0:0.06"/>
                                        <DoubleAnimation To="0" Storyboard.TargetName="buttonUIEffect" Storyboard.TargetProperty="ShadowDepth" Duration="0:0:0.06"/>
                                        <DoubleAnimation To="0" Storyboard.TargetName="buttonUIEffect" Storyboard.TargetProperty="Opacity" Duration="0:0:0.02"/>
                                        <DoubleAnimation To="0.98" Storyboard.TargetName="buttonUITransform" Storyboard.TargetProperty="(ScaleTransform.ScaleX)" Duration="0:0:0.1">
