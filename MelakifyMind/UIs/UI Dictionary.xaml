    <Style TargetType="{x:Type PasswordBox}">
        <Setter Property="Background" Value="{x:Null}"/>
        <Setter Property="KeyboardNavigation.TabNavigation" Value="None"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="BorderBrush" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="PasswordBox">
                    <Border Name="textBoxUIMain" BorderThickness="1" BorderBrush="sc#0.12, 0, 0, 0" Background="sc#0.5, 0.9646863, 0.9646863, 0.9646863" CornerRadius="6">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused">
                                    <Storyboard>
                                        <ColorAnimation To="#A31269A0" Duration="0:0:0.5" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                            <ColorAnimation.EasingFunction>
                                                <ExponentialEase Exponent="7" EasingMode="EaseOut"/>
                                            </ColorAnimation.EasingFunction>
                                        </ColorAnimation>
                                        <ColorAnimation To="White" Duration="0:0:0.5" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)">
                                            <ColorAnimation.EasingFunction>
                                                <ExponentialEase Exponent="7" EasingMode="EaseOut"/>
                                            </ColorAnimation.EasingFunction>
                                        </ColorAnimation>
                                        <ThicknessAnimation To="2" Storyboard.TargetName="textBoxUIMain" Storyboard.TargetProperty="BorderThickness" Duration="0:0:0.5">
                                            <ThicknessAnimation.EasingFunction>
                                                <ExponentialEase Exponent="7" EasingMode="EaseOut"/>
                                            </ThicknessAnimation.EasingFunction>
                                        </ThicknessAnimation>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Unfocused">
                                    <Storyboard>
                                        <ColorAnimation To="sc#0.12, 0, 0, 0" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" Duration="0:0:0.5">
                                            <ColorAnimation.EasingFunction>
                                                <ExponentialEase Exponent="7" EasingMode="EaseOut"/>
                                            </ColorAnimation.EasingFunction>
                                        </ColorAnimation>
                                        <ColorAnimation To="{Binding ElementName=textBoxUIMain, Path=Background}" Duration="0:0:0.5" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)">
                                            <ColorAnimation.EasingFunction>
                                                <ExponentialEase Exponent="7" EasingMode="EaseOut"/>
                                            </ColorAnimation.EasingFunction>
                                        </ColorAnimation>
                                        <ThicknessAnimation To="{Binding ElementName=textBoxUIMain, Path=BorderThickness}" Storyboard.TargetName="textBoxUIMain" Storyboard.TargetProperty="BorderThickness" Duration="0:0:0.5">
                                            <ThicknessAnimation.EasingFunction>
                                                <ExponentialEase Exponent="7" EasingMode="EaseOut"/>
                                            </ThicknessAnimation.EasingFunction>
                                        </ThicknessAnimation>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid>
                            <TextBlock Name="textBlockPlaceHolder" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="{TemplateBinding FontSize}" FontFamily="{TemplateBinding FontFamily}" Foreground="Gray" Text="{TemplateBinding Tag}" Margin="5,0,0,0"/>
                            <ScrollViewer x:Name="PART_ContentHost" FontSize="{TemplateBinding FontSize}" SnapsToDevicePixels="True" FontFamily="{TemplateBinding FontFamily}" Padding="{TemplateBinding Padding}" Foreground="{TemplateBinding Foreground}">
                            </ScrollViewer>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
