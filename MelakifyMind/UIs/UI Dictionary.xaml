                                        <EventTrigger RoutedEvent="ComboBox.MouseLeave">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Storyboard.TargetName="comboBoxUI" To="#FFFfffff" Duration="0:0:0.02" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"/>
                                                    <DoubleAnimation To="0.16" Storyboard.TargetName="comboBoxUIEffect" Storyboard.TargetProperty="Opacity" Duration="0:0:0.02"/>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                        <EventTrigger RoutedEvent="Button.PreviewMouseLeftButtonDown">
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <ColorAnimation Storyboard.TargetName="comboBoxUI" To="#FFF8F8F8" Duration="0:0:0.02" Storyboard.TargetProperty="(Border.Background).(SolidColorBrush.Color)"/>
                                                    <DoubleAnimation To="0" Storyboard.TargetName="comboBoxUIEffect" Storyboard.TargetProperty="Opacity" Duration="0:0:0.02"/>
                                                    <ThicknessAnimationUsingKeyFrames Storyboard.TargetName="comboBoxUIImageMain" Storyboard.TargetProperty="Margin" Duration="0:0:0.8">
                                                        <EasingThicknessKeyFrame KeyTime="0:0:0.2" Value="8,4,8,12">
                                                            <EasingThicknessKeyFrame.EasingFunction>
                                                                <ExponentialEase Exponent="7" EasingMode="EaseOut"/>
                                                            </EasingThicknessKeyFrame.EasingFunction>
                                                        </EasingThicknessKeyFrame>
                                                        <EasingThicknessKeyFrame KeyTime="0:0:0.35" Value="8,14,8,2">
                                                            <EasingThicknessKeyFrame.EasingFunction>
                                                                <ExponentialEase Exponent="7" EasingMode="EaseOut"/>
                                                            </EasingThicknessKeyFrame.EasingFunction>
                                                        </EasingThicknessKeyFrame>

                                                        <EasingThicknessKeyFrame KeyTime="0:0:0.8" Value="8">
                                                            <EasingThicknessKeyFrame.EasingFunction>
                                                                <ExponentialEase Exponent="7" EasingMode="EaseOut"/>
                                                            </EasingThicknessKeyFrame.EasingFunction>
                                                        </EasingThicknessKeyFrame>
                                                    </ThicknessAnimationUsingKeyFrames>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                        <EventTrigger RoutedEvent="Button.PreviewMouseLeftButtonUp">
