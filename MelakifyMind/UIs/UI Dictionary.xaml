                                        <DoubleAnimation To="0" Storyboard.TargetName="buttonUIEffect" Storyboard.TargetProperty="Opacity" Duration="0:0:0.02"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border.RenderTransform>
                            <ScaleTransform x:Name="buttonUITransform" ScaleX="1" ScaleY="1"/>
