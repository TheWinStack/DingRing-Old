                                                <ColumnDefinition Width="5*"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>

                                            <Image SnapsToDevicePixels="False" Grid.Column="1" Name="comboBoxUIImageMain" Margin="8" Stretch="None" OpacityMask="Black">
                                                <Image.Source>
                                                    <DrawingImage>
                                                        <DrawingImage.Drawing>
                                                            <GeometryDrawing>
                                                                <GeometryDrawing.Brush>
                                                                    <VisualBrush Stretch="None">
                                                                        <VisualBrush.Visual>
                                                                            <TextBlock FontFamily="SystemUI/#Segoe Fluent Icons" Text="&#xE70D;" FontSize="16">

                                                                            </TextBlock>
                                                                        </VisualBrush.Visual>
                                                                    </VisualBrush>
                                                                </GeometryDrawing.Brush>
                                                                <GeometryDrawing.Geometry>
                                                                    <RectangleGeometry Rect="20,20,20,20"/>
                                                                </GeometryDrawing.Geometry>
                                                            </GeometryDrawing>
                                                        </DrawingImage.Drawing>
                                                    </DrawingImage>
                                                </Image.Source>
                                            </Image>
                                        </Grid>
                                    </Border>
                                </ControlTemplate>
                            </ToggleButton.Template>
